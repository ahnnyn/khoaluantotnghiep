{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "config/*": ["./src/config/*"],
      "controllers/*": ["./src/controllers/*"],
      "models/*": ["./src/models/*"],
      "routes/*": ["./src/routes/*"],
      "services/*": ["./src/services/*"],
      "middleware/*": ["./src/middleware/*"],
      "utils/*": ["./src/utils/*"],
      "constants/*": ["./src/constants/*"],
      "templates/*": ["./src/templates/*"],
    },
    
  }
}
